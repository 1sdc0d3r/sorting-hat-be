{
  "name": "sorting-hat-be",
  "version": "1.0.0",
  "description": "RESTful NodeJS Backend for Tech Sorting Hat",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=staging jest --watch --runInBand",
    "server": "nodemon index.js",
    "start": "node index.js",
    "coverage": "cross-env NODE_ENV=staging jest --coverage --detectOpenHandles --forceExit || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lambda-School-Labs/sorting-hat-be.git"
  },
  "keywords": [],
  "author": "Aaron Janovitch, Arin Ramer, Braden Bell, Jacob Prudent & Tomas Ortiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lambda-School-Labs/sorting-hat-be/issues"
  },
  "homepage": "https://github.com/Lambda-School-Labs/sorting-hat-be#readme",
  "devDependencies": {
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./setup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/setup.js/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/database/**",
      "!**/tests/**",
      "!**/spec/**",
      "!**/*.spec.{js,jsx}"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.10",
    "knex-cleaner": "^1.3.0",
    "pg": "^7.18.2"
  }
}
